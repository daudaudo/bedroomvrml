DEF SCRIPT Script {
   eventIn SFBool clicked
   eventOut SFColor color
   field SFBool lit FALSE
   url "vrmlscript:

function clicked(val) {
   if(val) {
      if(lit) { 
	 color = new SFColor(1,0,0); 
	 lit = FALSE;
      }
      else { 
	 color = new SFColor(0,0,1); 
	 lit = TRUE;
      }
   }
}"

}

ROUTE TOUCH.isActive TO SCRIPT.clicked
ROUTE TOUCH.touchTime TO  Src.set_startTime
ROUTE TOUCH.touchTime TO  Src.set_stopTime
ROUTE TOUCH.touchTime TO  screen.set_startTime
ROUTE TOUCH.touchTime TO screen.set_stopTime
